{
  "name": "@psol/sdk",
  "version": "0.4.0",
  "description": "TypeScript SDK for pSol Privacy Pool on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "privacy",
    "zk-snark",
    "groth16",
    "tornado",
    "mixer"
  ],
  "author": "pSol Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/psolprotocol/psol-v1"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@noble/hashes": "^1.3.3",
    "@solana/web3.js": "^1.95.0",
    "@solana/spl-token": "^0.4.0",
    "bn.js": "^5.2.1",
    "circomlibjs": "^0.1.7",
    "snarkjs": "^0.7.4"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/node": "^20.11.0",
    "@types/snarkjs": "^0.7.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": ">=0.29.0",
    "@solana/web3.js": ">=1.90.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
